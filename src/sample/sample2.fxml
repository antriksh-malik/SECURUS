    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <Button fx:id="btn" layoutX="182.0" layoutY="333.0" mnemonicParsing="false" onAction="#nxt_screen" prefHeight="26.0" prefWidth="92.0" text="Go" />
            <Button fx:id="btn2" layoutX="29.0" layoutY="330.0" mnemonicParsing="false" onAction="#retry" prefHeight="26.0" prefWidth="92.0" text="Retry" />
